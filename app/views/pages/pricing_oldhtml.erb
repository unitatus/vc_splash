	            </div>
	            <div id="box2" style="display:none">
					<%= render :partial => "pricing_field", :locals => { :field_name => "field1", :storage_discount => @cust_box_count_1_months_2_disc, :inventory_discount => @cust_inventorying_count_1_months_2_disc, :fb_incrementer => fb_incrementer += 1 } %>
					<%= render :partial => "pricing_field", :locals => { :field_name => "field2", :storage_discount => @cust_box_count_2_months_2_disc, :inventory_discount => @cust_inventorying_count_2_months_2_disc, :fb_incrementer => fb_incrementer += 1 } %>
					<%= render :partial => "pricing_field", :locals => { :field_name => "field3", :storage_discount => @cust_box_count_3_months_2_disc, :inventory_discount => @cust_inventorying_count_3_months_2_disc, :fb_incrementer => fb_incrementer += 1 } %>
	            </div>
	            <div id="box3" style="display:none">
					<%= render :partial => "pricing_field", :locals => { :field_name => "field1", :storage_discount => @cust_box_count_1_months_1_disc, :inventory_discount => @cust_inventorying_count_1_months_1_disc, :fb_incrementer => fb_incrementer += 1 } %>
					<%= render :partial => "pricing_field", :locals => { :field_name => "field2", :storage_discount => @cust_box_count_2_months_1_disc, :inventory_discount => @cust_inventorying_count_2_months_1_disc, :fb_incrementer => fb_incrementer += 1 } %>
					<%= render :partial => "pricing_field", :locals => { :field_name => "field3", :storage_discount => @cust_box_count_3_months_1_disc, :inventory_discount => @cust_inventorying_count_3_months_1_disc, :fb_incrementer => fb_incrementer += 1 } %>
	            </div>
	            <div id="box4" style="display:none">
					<%= render :partial => "pricing_field", :locals => { :field_name => "field2", :storage_discount => @cust_box_no_discount_disc, :inventory_discount => @cust_inv_no_discount_disc, :fb_incrementer => fb_incrementer += 1  } %>
	            </div>
	        </div>
	    </div>
	<div style="padding-left: 150px"></div>
	<div style="display: none">
	<div id="cf_explainer" style="text-align: center">You pay by the cubic foot for your boxes, or by the box for Visible Closet boxes.</div>
	</div>